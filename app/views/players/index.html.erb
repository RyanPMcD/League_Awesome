<h1>Players#index</h1>
<p>Find me in app/views/players/index.html.erb</p>
<table>
	<thead>
		<tr>
			<th>Player Name</th>
			<th>Position</th>
			<th>NFL Team</th>
		</tr>
	</thead>
	<tbody>
		<% @players.each do |player|%>
		<tr>
			<td><%= player.name %></td>
			<td><%= player.position %></td>
			<td><%= player.nfl_team %></td>
		</tr>
		<% end %>
	</tbody>
</table>